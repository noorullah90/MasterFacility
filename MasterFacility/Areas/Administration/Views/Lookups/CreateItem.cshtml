@model MasterFacility.Data.Models.Lookups.lookupitem

@{
    ViewData["Title"] = "CreateItem";
}

<h1>CreateItem</h1>

<h4>lookupitem</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="CreateItem">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="lookupid" class="control-label"></label>
                <select asp-for="lookupid" data-toggle="select2"  class="form-control select2" ></select>
                <span asp-validation-for="lookupid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="hmisid" class="control-label"></label>
                <input asp-for="hmisid" class="form-control" />
                <span asp-validation-for="hmisid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="name" class="control-label"></label>
                <input asp-for="name" class="form-control" />
                <span asp-validation-for="name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="namedari" class="control-label"></label>
                <input asp-for="namedari" class="form-control" />
                <span asp-validation-for="namedari" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="namepashto" class="control-label"></label>
                <input asp-for="namepashto" class="form-control" />
                <span asp-validation-for="namepashto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="parentid" class="control-label"></label>
                <select asp-for="parentid" class="form-control select2" ></select>
                <span asp-validation-for="parentid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="order" class="control-label"></label>
                <input asp-for="order" class="form-control" />
                <span asp-validation-for="order" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="isactive" /> @Html.DisplayNameFor(model => model.isactive)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

